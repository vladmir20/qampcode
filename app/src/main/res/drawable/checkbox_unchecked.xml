<?xml version="1.0" encoding="utf-8"?>
<!-- checkbox_unchecked.xml -->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/white"/> <!-- White background -->
            <corners android:radius="4dp"/> <!-- Corner radius of 4dp -->
            <stroke android:width="2dp" android:color="#828282"/> <!-- Black border -->
        </shape>
    </item>
    <item android:drawable="@drawable/ic_check_icon"
        android:gravity="center" />
</layer-list>
